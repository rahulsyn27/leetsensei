{
    "manifest_version": 3,
    "name": "LeetSensei",
    "version": "0.1.0",
    "description": "An AI assistant to help you solve LeetCode problems.",
    "permissions": [
      "storage",
      "activeTab"
    ],
    "host_permissions": [
      "https://leetcode.com/problems/*"
    ],
    "background": {
      "service_worker": "background.js",
      "type": "module" 
    },
    "content_scripts": [
      {
        "matches": ["https://leetcode.com/problems/*"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "icons": {
      "128": "icon.png"
    }
  }